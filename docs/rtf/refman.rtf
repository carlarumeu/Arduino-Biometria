00290   {\cf18 void} emitirAnuncioIBeaconLibre( {\cf17 const} {\cf18 char} * carga, {\cf17 const} uint8_t tamanyoCarga ) \{\par
00291 \par
00292   (*this).detenerAnuncio(); \par
00293 \par
00294   Bluefruit.Advertising.clearData();\par
00295   Bluefruit.ScanResponse.clearData(); {\cf20 // hace falta?}\par
00296 \par
00297   {\cf20 // Bluefruit.setTxPower( (*this).txPower ); creo que no lo pongo porque es uno de los bytes de la parte de carga que utilizo}\par
00298   Bluefruit.setName( (*this).nombreEmisora );\par
00299   Bluefruit.ScanResponse.addName();\par
00300 \par
00301   Bluefruit.Advertising.addFlags(BLE_GAP_ADV_FLAGS_LE_ONLY_GENERAL_DISC_MODE);\par
00302 \par
00303   {\cf20 // con este parece que no va  !}\par
00304   {\cf20 // Bluefruit.Advertising.addFlags(BLE_GAP_ADV_FLAG_LE_GENERAL_DISC_MODE);}\par
00305 \par
00306   {\cf20 //}\par
00307   {\cf20 // hasta ahora habr\'E1, supongo, ya puestos los 5 primeros bytes. Efectivamente.}\par
00308   {\cf20 // Falta poner 4 bytes fijos (company ID, beacon type, longitud) y 21 de carga}\par
00309   {\cf20 //}\par
00310   uint8_t restoPrefijoYCarga[4+21] = \{\par
00311     0x4c, 0x00, {\cf20 // companyID 2}\par
00312     0x02, {\cf20 // ibeacon type 1byte}\par
00313     21, {\cf20 // ibeacon length 1byte (dec=21)  longitud del resto // 0x15 // ibeacon length 1byte (dec=21)  longitud del resto}\par
00314     {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, \par
00315     {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, \par
00316     {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, \par
00317     {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, \par
00318     {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, {\cf23 '-'}, \par
00319     {\cf23 '-'}\par
00320   \};\par
00321 \par
00322   {\cf20 //}\par
00323   {\cf20 // addData() hay que usarlo s\'F3lo una vez. Por eso copio la carga}\par
00324   {\cf20 // en el anterior array, donde he dejado 21 sitios libres}\par
00325   {\cf20 //}\par
00326   memcpy( &restoPrefijoYCarga[4], &carga[0], ( tamanyoCarga > 21 ? 21 : tamanyoCarga ) ); \par
00327 \par
00328   {\cf20 //}\par
00329   {\cf20 // copio la carga para emitir}\par
00330   {\cf20 //}\par
00331   Bluefruit.Advertising.addData( BLE_GAP_AD_TYPE_MANUFACTURER_SPECIFIC_DATA, &restoPrefijoYCarga[0], 4+21 );\par
00332 \par
00333   {\cf20 //}\par
00334   {\cf20 // ? qu\'E9 valores poner aqu\'ED ?}\par
00335   {\cf20 //}\par
00336   Bluefruit.Advertising.restartOnDisconnect({\cf17 true});\par
00337   Bluefruit.Advertising.setInterval(100, 100);    {\cf20 // in unit of 0.625 ms}\par
00338 \par
00339   Bluefruit.Advertising.setFastTimeout( 1 );      {\cf20 // number of seconds in fast mode}\par
00340   {\cf20 //}\par
00341   {\cf20 // empieza el anuncio, 0 = tiempo indefinido (ya lo parar\'E1n)}\par
00342   {\cf20 //}\par
00343   Bluefruit.Advertising.start( 0 ); \par
00344 \par
00345   Globales::elPuerto.escribir( {\cf22 "emitiriBeacon libre  Bluefruit.Advertising.start( 0 );  \\n"});\par
00346   \} {\cf20 // ()}\par
